{
  "name": "functions",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./lib",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "seed": "npm run build && node ./lib/test/dbseed.js",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "dev": "npm run build && firebase emulators:start --import=./local_data --export-on-exit"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
