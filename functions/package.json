    "algoliasearch": "^4.13.1",
    "body-parser": "^1.20.0",
    "chance": "^1.1.8",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.3",
    "err-code": "^3.0.1",
    "express": "^4.18.1",
    "express-json-validator-middleware": "^2.2.1",
    "firebase": "^9.8.4",
    "firebase-admin": "^11.0.0",
    "firebase-functions": "^3.22.0",
    "getstream": "^7.4.1",
    "helmet": "^4.6.0",
    "human-password": "^1.0.4",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "swagger-jsdoc": "^6.2.1",
